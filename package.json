{
  "name": "new-tab-identity",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && web-ext build -s ./dist -a ./dist --overwrite-dest",
    "lint": "tsc && eslint . --ext ts,tsx && npm run build && web-ext lint -s ./dist",
    "test": "vitest",
    "start": "web-ext run --source-dir ./dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@donovanglover/base16-tailwind": "npm:@jsr/donovanglover__base16-tailwind@^0.1.6",
    "@types/firefox-webext-browser": "^120.0.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "eslint-config-love": "^47.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.11",
    "vitest": "^1.5.3",
    "web-ext": "^7.11.0"
  }
}
