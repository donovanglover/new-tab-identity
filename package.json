{
  "name": "new-tab-identity",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "build": "npm run build:vite && npm run build:web-ext",
    "build:vite": "vite build",
    "build:web-ext": "web-ext build -s ./dist --overwrite-dest",
    "lint": "npm run lint:tsc && npm run lint:eslint && npm run lint:web-ext",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:web-ext": "npm run build && web-ext lint -s ./dist",
    "test": "vitest",
    "start": "web-ext run --source-dir ./dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "^120.0.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.6.0",
    "eslint-config-love": "^47.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.5.3",
    "web-ext": "^7.11.0"
  }
}
