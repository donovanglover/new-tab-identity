{
  "manifest_version": 2,
  "name": "New Tab Identity",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_title": "New Tab Identity",
    "default_popup": "index.html",
    "default_icon": {
      "128": "icon.svg"
    }
  },

  "options_ui": {
    "page": "index.html"
  },

  "permissions": [
    "contextualIdentities",
    "cookies",
    "proxy",
    "storage",
    "<all_urls>"
  ],

  "icons": {
    "128": "icon.svg"
  },

  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "91.1.0",
      "id": "some-name@example.org"
    }
  }
}
